{
  "api": {
    "myapp": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7,
              "description": "for myapp developersIO handson https://dev.classmethod.jp/articles/react-amplify-appsync-dynamodb-tutorial/"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  },
  "hosting": {
    "S3AndCloudFront": {
      "service": "S3AndCloudFront",
      "providerPlugin": "awscloudformation"
    }
  }
}